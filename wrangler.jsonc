{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "education",
  "main": "api/index.js",
  "compatibility_flags": ["nodejs_compat"],
  "compatibility_date": "2025-04-01",
  "observability": {
    "enabled": true
  },
  "assets": {
    "binding": "ASSETS",
    "not_found_handling": "single-page-application"
  },
  "routes": [
    {
      "pattern": "thnk.edu.vn",
      "custom_domain": true
    }
  ],
  "placement": {
    "mode": "smart"
  },
  "upload_source_maps": true
}
